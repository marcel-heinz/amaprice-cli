{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "installCommand": "npm install --ignore-scripts --omit=prod && cd website && npm install",
  "buildCommand": "cd website && npm run build",
  "devCommand": "cd website && npm run dev",
  "cleanUrls": true,
  "trailingSlash": false
}
